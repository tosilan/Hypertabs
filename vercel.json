{
  "regions": ["hnd1"],
  "builds": [
    { "src": "src/index.js","use": "@vercel/node" },
    { "src": "static/**","use": "@vercel/static" },
    { "src": "*.json","use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/","dest": "/static/index.html" },
    { "src": "/uv/service/(.*)","dest": "/static/uv/$1" },
    { "src": "/.*","dest": "src/index.js" }
  ],
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/.*", "dest": "app.js" }
  ]
}
